easyblock = 'CMakeMake'

name = 'kallisto'
version = "0.46.0"

homepage = 'http://pachterlab.github.io/kallisto/'
description = """kallisto is a program for quantifying abundances of transcripts from RNA-Seq data, or more generally
 of target sequences using high-throughput sequencing reads."""

toolchain = {'version': '2018.3', 'name': 'iccifort'}
toolchainopts = {'pic': True}

sources = ['v%(version)s.tar.gz']
source_urls = ['https://github.com/pachterlab/%(name)s/archive/']

builddependencies = [
    ('CMake', '3.9.1'),
]
dependencies = [
    ('zlib', '1.2.11'),
    ('HDF5', '1.8.18'),
]

configopts = " -DZLIB_ROOT=$NIXUSER_PROFILE "

#separate_build_dir = True
prebuildopts = ['cd ext/htslib && autoheader && autoconf && cd ../.. &&']

parallel = 1


sanity_check_paths = {
    'files': ['bin/%(name)s'],
    'dirs': [],
}

moduleclass = 'bio'
